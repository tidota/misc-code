   "source": [
    "# Binary file inspection\n",
    "\n",
    "# objdump\n",
    "\n",
    "## show assembly code without instruction bytes\n",
    "```\n",
    "objdump --no-show-raw-insn -d <binary file>\n",
    "```\n",
    "\n",
    "## does the same thing but elf-specific\n",
    "```\n",
    "readelf -a <binary file>\n",
    "```\n"
   ]
  }
